<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Establece una gama de colores personalizables solo se cambia lo que esta dentro
        de la propiedad Color-->
    <SolidColorBrush x:Key="c-primary" Color="#AC1419" />
    <SolidColorBrush x:Key="c-secondary" Color="#CC8C12"/>
    <SolidColorBrush x:Key="c-success" Color="#55814D" />
    <SolidColorBrush x:Key="c-info" Color="#397FB5" />
    <SolidColorBrush x:Key="c-warning" Color="#D4A440" />
    <SolidColorBrush x:Key="c-danger" Color="#B41F20" />
    <SolidColorBrush x:Key="c-light-emphasis" Color="#FFDFDFDF" />
    <SolidColorBrush x:Key="c-dark-emphasis" Color="#121212" />
    <SolidColorBrush x:Key="c-light" Color="#FFFFFF" />
    <SolidColorBrush x:Key="c-dark" Color="#000000" />
</ResourceDictionary>